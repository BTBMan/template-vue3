{
  "name": "[name]",
  "description": "A Vue3 template with Vite, Eslint, Prettier, Stylelint, Vitest, Husky, Commitlint, Postcss",
  "version": "0.0.0",
  "scripts": {
    "serve": "npm run dev",
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview --port 5050",
    "preview:build": "npm run build && npm run preview",
    "type:check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "prepare": "husky install",
    "prettier": "prettier --no-editorconfig --config-precedence prefer-file \"**/*.{js,cjs,mjs,ts,cts,mts,json,jsonc,jsx,tsx,css,less,scss,styl,vue,html,md}\"",
    "lint:eslint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts",
    "lint:prettier": "npm run prettier -- --check",
    "lint:stylelint": "stylelint \"**/*.{css,scss,less,styl,postcss,vue,html}\"",
    "lint:lint-staged": "lint-staged",
    "fix:eslint": "npm run lint:eslint -- --fix",
    "fix:prettier": "npm run prettier -- --write",
    "fix:stylelint": "npm run lint:stylelint -- --fix",
    "commit": "node ./node_modules/cz-customizable/standalone.js",
    "test:unit": "vitest --environment jsdom"
  },
  "dependencies": {
    "@ant-design/icons-vue": "^6.0.1",
    "@vueuse/core": "^7.7.1",
    "ant-design-vue": "^2.2.8",
    "axios": "^0.26.0",
    "dayjs": "^1.10.8",
    "lodash-es": "^4.17.21",
    "pinia": "^2.0.11",
    "vue": "^3.2.31",
    "vue-router": "^4.0.12"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.2.1",
    "@rushstack/eslint-patch": "^1.1.0",
    "@types/jsdom": "^16.2.14",
    "@types/lodash-es": "^4.17.6",
    "@types/node": "^16.11.26",
    "@vitejs/plugin-vue": "^2.2.2",
    "@vitejs/plugin-vue-jsx": "^1.3.7",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^10.0.0",
    "@vue/test-utils": "^2.0.0-rc.18",
    "@vue/tsconfig": "^0.1.3",
    "autoprefixer": "^10.4.2",
    "commitlint-config-cz": "^0.13.3",
    "cz-customizable": "^6.3.0",
    "eslint": "^8.5.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-vue": "^8.2.0",
    "husky": "^7.0.4",
    "jsdom": "^19.0.0",
    "less": "^4.1.2",
    "lint-staged": "^12.3.5",
    "postcss": "^8.4.8",
    "postcss-html": "^1.3.0",
    "postcss-less": "^6.0.0",
    "postcss-pxtorem": "^6.0.0",
    "prettier": "^2.5.1",
    "stylelint": "^14.5.3",
    "stylelint-config-html": "^1.0.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-standard": "^25.0.0",
    "stylelint-order": "^5.0.0",
    "typescript": "^4.6.2",
    "unplugin-auto-import": "^0.6.1",
    "unplugin-vue-components": "^0.17.21",
    "vite": "^2.9.4",
    "vite-plugin-eslint": "^1.3.0",
    "vite-plugin-iconfont-css": "^0.1.1",
    "vite-plugin-optimize-persist": "^0.1.2",
    "vite-plugin-package-config": "^0.1.1",
    "vite-plugin-stylelint": "^1.2.0",
    "vitest": "^0.5.0",
    "vue-tsc": "^0.31.4"
  },
  "vite": {
    "optimizeDeps": {
      "include": [
        "@ant-design/icons-vue",
        "ant-design-vue",
        "ant-design-vue/es",
        "ant-design-vue/es/badge/style",
        "ant-design-vue/es/button/style",
        "ant-design-vue/es/col/style",
        "ant-design-vue/es/config-provider/style",
        "ant-design-vue/es/config-provider/style/css",
        "ant-design-vue/es/form/style",
        "ant-design-vue/es/input/style",
        "ant-design-vue/es/layout/style",
        "ant-design-vue/es/layout/style/css",
        "ant-design-vue/es/locale/zh_CN",
        "ant-design-vue/es/menu/style",
        "ant-design-vue/es/pagination/style",
        "ant-design-vue/es/popover/style",
        "ant-design-vue/es/progress/style",
        "ant-design-vue/es/radio/style",
        "ant-design-vue/es/row/style",
        "ant-design-vue/es/select/style",
        "ant-design-vue/es/switch/style",
        "ant-design-vue/es/table/style",
        "ant-design-vue/es/tag/style",
        "axios",
        "lodash-es",
        "vue",
        "vue-router"
      ]
    }
  }
}
